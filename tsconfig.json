{
  "compilerOptions": {
    "target": "es5",                               
    "module": "commonjs",
    "lib": ["ES7"],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true                    
  }
}
